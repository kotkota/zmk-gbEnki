ba: ba {
    compatible = "zmk,behavior-macro";
    #binding-cells = <0>;
    bindings =
        <&ru_off>,
        <&macro_wait_time 20>,
        <&kp B &kp A &kp C &kp K &kp G &kp R &kp O &kp U &kp N &kp D &kp COLON &kp SPACE>;

    label = "BA";
};

